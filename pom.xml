<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>RedProtect</groupId>
  <artifactId>RedProtect</artifactId>
  <packaging>jar</packaging>
  <name>RedProtect</name>
  <dependencies>
		<!--Spigot-API-->
        <dependency>
          <groupId>org.spigotmc</groupId>
          <artifactId>spigot-api</artifactId>
          <version>1.8.6-R0.1-SNAPSHOT</version>
        </dependency>	
		<dependency>
			<groupId>BossbarAPI</groupId>
			<artifactId>BossbarAPI</artifactId>
			<version>v1</version>
			<scope>system</scope>
			<systemPath>D:/ProjetosBukkit/APIs/BossbarAPI.jar</systemPath>
		</dependency>
		<dependency>
			<groupId>MyChunk</groupId>
			<artifactId>MyChunk</artifactId>
			<version>v1</version>
			<scope>system</scope>
			<systemPath>D:/ProjetosBukkit/APIs/MyChunk.jar</systemPath>
		</dependency>
		<dependency>
			<groupId>MyPet</groupId>
			<artifactId>MyPet</artifactId>
			<version>v1</version>
			<scope>system</scope>
			<systemPath>D:/ProjetosBukkit/APIs/MyPet-1.8-1.2.1-SNAPSHOT.jar</systemPath>
		</dependency>
		<dependency>
			<groupId>McMMo</groupId>
			<artifactId>McMMo</artifactId>
			<version>v1</version>
			<scope>system</scope>
			<systemPath>D:/ProjetosBukkit/APIs/McMMo.jar</systemPath>
		</dependency>
		<dependency>
			<groupId>SkillApi</groupId>
			<artifactId>SkillApi</artifactId>
			<version>v1</version>
			<scope>system</scope>
			<systemPath>D:/ProjetosBukkit/APIs/SkillApi.jar</systemPath>
		</dependency>
		<dependency>
			<groupId>MagicCarpet</groupId>
			<artifactId>MagicCarpet</artifactId>
			<version>v1</version>
			<scope>system</scope>
			<systemPath>D:/ProjetosBukkit/APIs/MagicCarpet.jar</systemPath>
		</dependency>
  </dependencies>
  <repositories>
     <repository>
            <id>spigot-repo</id>
            <url>https://hub.spigotmc.org/nexus/content/groups/public/</url>
     </repository>
  </repositories>
  <build>
    <finalName>${project.name}</finalName>
    <defaultGoal>clean package install</defaultGoal>
    <plugins>
        <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.1</version>
        <configuration>
          <source>1.7</source>
          <target>1.7</target>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <version>5.6</version>
</project>